<p id="notice"><%= notice %></p>

<h1>Computer Infos</h1>

<table>
  <thead>
    <tr>
	<th>Color</th>
	<th>Brand</th>
	<th>Processor</th>
      	<th>Room number</th>
	<th>Room size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computer_infos.each do |computer_info| %>
      <tr>
        <td><%= computer_info.color %></td>
        <td><%= computer_info.brand %></td>
        <td><%= computer_info.processor %></td>
        <td><%= computer_info.room.room_number %></td>
	<td><%= computer_info.room.room_size %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

